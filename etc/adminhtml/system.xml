<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="teeshter" translate="label" sortOrder="200">
            <label>Teeshter</label>
        </tab>
        <section id="teeshter_wholesale" translate="label" type="text" sortOrder="110"
                 showInDefault="1" showInWebsite="1" showInStore="1">
            <label>Wholesale</label>
            <tab>teeshter</tab>
            <resource>Teeshter_Wholesale::inquiry</resource>
            <group id="log" translate="label" type="text" sortOrder="110"
                   showInDefault="1" showInWebsite="1" showInStore="1">
                <label>Inquiry Log</label>
                <field id="enabled" translate="label" type="select" sortOrder="110"
                       showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Enabled</label>
                    <source_model>Magento\Config\Model\Config\Source\Enabledisable</source_model>
                </field>
            </group>
        </section>
    </system>
</config>
